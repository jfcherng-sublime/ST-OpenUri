            }, {
                "caption": "-",
            }, {
                "caption": "README",
                "command": "open_file",
                "args": {
                    "file": "${packages}/OpenUrlInBrowser/README.md",
                },
            }, {
                "caption": "CHANGELOG",
                "command": "open_file",
                "args": {
                    "file": "${packages}/OpenUrlInBrowser/CHANGELOG.md",
                },
            }, {
                "caption": "-",
            }]
        }]
    }]
}]
